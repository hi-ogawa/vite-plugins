{
  "name": "vite-puglins-example-ssr",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm --seq run /^build:/",
    "build:vite": "vite build",
    "build:inject-manifest": "node ./misc/inject-global-script.js dist/client/index.html dist/client/manifest.json __viteManifest",
    "build-vercel": "pnpm build && bash misc/vercel/build.sh",
    "build-preview": "pnpm build",
    "preview": "vite preview",
    "release": "vercel deploy --prebuilt .",
    "release-production": "vercel deploy --prebuilt . --prod"
  },
  "devDependencies": {
    "@hiogawa/utils": "1.6.0",
    "@hiogawa/vite-glob-routes": "workspace:*",
    "@hiogawa/vite-import-dev-server": "workspace:*",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0",
    "vite": "^4.4.4"
  }
}
