{
  "name": "@hiogawa/vite-glob-routes",
  "version": "0.6.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/react-router/client": {
      "import": "./dist/react-router/client.js",
      "require": "./dist/react-router/client.cjs",
      "types": "./dist/react-router/client.d.ts"
    },
    "./dist/react-router/server": {
      "import": "./dist/react-router/server.js",
      "require": "./dist/react-router/server.cjs",
      "types": "./dist/react-router/server.d.ts"
    },
    "./dist/hattip": {
      "import": "./dist/hattip.js",
      "require": "./dist/hattip.cjs",
      "types": "./dist/hattip.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hi-ogawa/vite-plugins",
    "directory": "packages/vite-glob-routes"
  },
  "scripts": {
    "build": "tsup",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@hattip/compose": "^0.0.34",
    "@types/react": "^18.2.14",
    "react-router": "^6.13.0",
    "react-router-dom": "^6.13.0",
    "vite": "^4.4.4"
  },
  "peerDependencies": {
    "@hattip/compose": "*",
    "@types/react": "*",
    "react-router": "*",
    "react-router-dom": "*",
    "vite": "*"
  },
  "peerDependenciesMeta": {
    "@hattip/compose": {
      "optional": true
    },
    "react-router": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    }
  }
}
