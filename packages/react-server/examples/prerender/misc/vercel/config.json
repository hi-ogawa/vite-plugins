{
  "version": 3,
  "routes": [
    {
      "src": "^/assets/(.*)$",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "src": "^(.*)",
      "has": [
        {
          "type": "query",
          "key": "__rsc"
        }
      ],
      "dest": "$1/index.data"
    },
    {
      "src": "^(.*)/index.data$",
      "headers": {
        "content-type": "text/x-component;charset=utf-8"
      }
    },
    {
      "handle": "filesystem"
    }
  ]
}
