{
  "name": "@hiogawa/vite-plugins-demo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "SERVER_ENTRY=./src/server/adapter-vercel-edge.ts pnpm run --sequential /^build:/",
    "build:vite": "vite build && vite build --ssr",
    "build:vercel": "bash misc/vercel/build.sh",
